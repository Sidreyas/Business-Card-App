{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/upload",
      "dest": "/api/upload-db.js"
    },
    {
      "src": "/api/entries",
      "dest": "/api/entries.js"
    },
    {
      "src": "/api/init-db",
      "dest": "/api/init-db.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "api/upload-db.js": {
      "maxDuration": 60
    },
    "api/init-db.js": {
      "maxDuration": 30
    }
  }
}
